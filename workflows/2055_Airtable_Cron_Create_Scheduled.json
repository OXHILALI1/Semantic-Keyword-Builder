      "parameters": {
        "operation": "send",
        "sendTo": "subscribers@example.com",
        "subject": "Weekly Newsletter",
        "emailType": "html"
      },
      "typeVersion": 1
    },
    {
      "id": "airtable-node",
      "name": "Get Airtable Data",
